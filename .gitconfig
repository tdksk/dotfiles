[user]
  name  = Keisuke Tada
  email = tdkskdt@gmail.com
[color]
  ui = true
[push]
  default = current
[alias]
  ad = add
  co = checkout
  ci = commit
  st = status
  br = branch
  ba = branch -a
  di = diff
  d = diff --stat
  dc = diff --cached
  dw = diff --word-diff=color
  pr = pull --rebase
  lg = log -p --stat
  tree = log --graph --pretty=oneline --abbrev-commit --decorate
[core]
  excludesfile = ~/.gitignore
